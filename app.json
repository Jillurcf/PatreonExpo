{
  "expo": {
    "name": "between",
    "slug": "between",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/betweenLogo.jpg",
    "scheme": "between",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.yxltechnology.between",
      "infoPlist": {
        "NSAppTransportSecurity": {
          "NSAllowsArbitraryLoads": true
        }
      }
    },
    "android": {
        "usesCleartextTraffic": true,
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/betweenLogo.jpg",
        "backgroundColor": "#ffffff"
      },
      "edgeToEdgeEnabled": true,
      "package": "com.yxltechnology.between"
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/betweenLogo.jpg"
    },
    "plugins": [
      "expo-router",
      [
        "expo-splash-screen",
        {
          "image": "./assets/images/betweenLogo.jpg",
          "imageWidth": 200,
          "resizeMode": "contain",
          "backgroundColor": "#ffffff"
        }
      ]
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "router": {},
      "eas": {
        "projectId": "80eb2b75-cf58-4b2d-8da2-27b46499769c"
      }
    }
  }
}
